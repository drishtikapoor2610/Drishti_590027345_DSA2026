#include <stdio.h>
//WAP TO CHECK WHETHER A NUMBER IS PRIME OR NOT
int main()
{
    int n, i;

    printf("Enter a number: ");
    scanf("%d", &n);

    if (n <= 1)
    {
        printf("Not Prime");
        return 0;
    }

    for (i = 2; i < n; i++)
    {
        if (n % i == 0)
        {
            printf("Not Prime");
            return 0;
        }
    }

    printf("Prime");
    return 0;
}

