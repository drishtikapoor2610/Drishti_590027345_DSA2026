#include <stdio.h>
//WAP TO SWAP THE VALUES OF 2 NO USING TEMP VARIABLE.
int main() {
    int a, b, temp;

    // Input values
    printf("Enter two numbers:\n");
    scanf("%d %d", &a, &b);

    // Swapping using temporary variable
    temp = a;
    a = b;
    b = temp;

    // Output after swapping
    printf("After swapping:\n");
    printf("a = %d\n", a);
    printf("b = %d\n", b);

    return 0;
}
