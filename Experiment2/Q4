#include <stdio.h>
#include <stdlib.h>

struct Node {
    int data;
    struct Node *next;
};

void display(struct Node *head) {
    if(head == NULL) {
        printf("List is empty.\n");
        return;
    }

    printf("Linked List: ");
    while(head != NULL) {
        printf("%d -> ", head->data);
        head = head->next;
    }
    printf("NULL\n");
}

void insertEnd(struct Node **head, int value) {
    struct Node *newNode = (struct Node*)malloc(sizeof(struct Node));
    newNode->data = value;
    newNode->next = NULL;

    if(*head == NULL) {
        *head = newNode;
        return;
    }

    struct Node *temp = *head;
    while(temp->next != NULL)
        temp = temp->next;

    temp->next = newNode;
}

void deleteBeginning(struct Node **head) {
    if(*head == NULL) {
        printf("List is already empty.\n");
        return;
    }

    struct Node *temp = *head;
    *head = temp->next;
    free(temp);

    printf("First node deleted successfully.\n");
}

