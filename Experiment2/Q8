#include <stdio.h>
#include <stdlib.h>

struct DCNode {
    int value;
    struct DCNode *prev;
    struct DCNode *next;
};

void insertStart(struct DCNode **head, int val) {
    struct DCNode *node = malloc(sizeof(struct DCNode));
    node->value = val;

    if (*head == NULL) {
        node->next = node;
        node->prev = node;
        *head = node;
        return;
    }

    struct DCNode *last = (*head)->prev;

    node->next = *head;
    node->prev = last;
    last->next = node;
    (*head)->prev = node;
    *head = node;
}
